void main():
	@Dog john = @Dog('John', 'Black', 'Puddle', 5)
	@Dog gerome = @Dog('Gerome', 'Yellow', 'Golden Retriever', 3)
	array<@Dog> dogs = [<@Dog, 0, null>]
	dogs.append(john)
	dogs.append(gerome)
	for dog in dogs:
		dog.bark()
	end

end


class @Dog:
	string name
	string color
	string breed
	uint age

	void Dog(string name, string color, string breed, uint age):
		self.name = name	
		self.color = color	
		self.breed = breed	
		self.age = age	
	end

	void bark():
		print('Woof! here are my details--------')
		print('Name: ' + self.name)
		print('Color: ' + self.color)
		print('Breed: ' + self.breed)
		print(self.age)
		print('--------------------------------')
		print('')
	end

end

