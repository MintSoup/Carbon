void main():
	polymorph('lol')
	polymorph([3, 5, 2])
	polymorph([3..6])
	polymorph(main)
end

void polymorph(object o):
	if o is string:
		print('Your string was ' + (string) o)
	elif o is array<uint>:
		print(o)
	elif o is generator<uint>:
		for g in (generator<uint>) o:
			print(g * 3)	
		end
	else:
		print('Unknown Type')
	end
end
