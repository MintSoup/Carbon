void main():
	polymorph(main)
	print(3)

	int g = 3.2 as int
	print(g)
end

void polymorph(object o):
	if o is string:
		print('Your string was ' + o as string)
	elif o is array<uint>:
		print(o)
	elif o is generator<uint>:
		for g in o as generator<uint>:
			print(g * 3)	
		end
	else:
		print('Unknown Type')
	end
end
